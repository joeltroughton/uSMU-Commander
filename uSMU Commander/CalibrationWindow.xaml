<Window x:Class="uSMU_Commander.CalibrationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:uSMU_Commander"
        mc:Ignorable="d"
        Title="uSMU calibration" Height="207" Width="300">
    <Grid>
        <StackPanel>
            <TextBox x:Name="calibrationSettings" Height="130"/>
            <Button x:Name="exportCalButton" Content="Export calibration data" Click="exportCalButton_Click"/>
            <Button x:Name="importCalButton" Content="Import calibration data" Click="importCalButton_Click"/>
        </StackPanel>
    </Grid>
</Window>
